{
    "name": "@scope/airtable",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./core": {
            "types": "./dist/core/index.d.ts",
            "default": "./dist/core/index.js"
        },
        "./server": {
            "types": "./dist/server/index.d.ts",
            "default": "./dist/server/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "devDependencies": {
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    }
}
