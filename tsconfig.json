{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2022", "DOM"],
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "strict": true,
      "declaration": true,
      "declarationMap": true,
      "emitDeclarationOnly": false,
      "removeComments": false,
      "sourceMap": true,
      "noEmit": true,
      "skipLibCheck": true,
      "verbatimModuleSyntax": true
    },
    "include": ["src"]
  }
  